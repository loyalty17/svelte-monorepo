{
	"pipeline": {

		"build": {
      "outputs": ["build/**", ".svelte-kit/**", "svelte-kit/**"],
			"dependsOn": ["^build"]
		},

		"lint": {
			"outputs": []
		},

		"test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": []
    },

		"dev": {
			"cache": false
		},

		"preview": {
			"cache": true
		},

		"package": {
			"outputs": ["package"]
		}

	}
}
