{
	"name": "sveltekit-turborepo",
	"description": "PNPM + Turborepo + Sveltekit",
	"version": "0.2.0",
	"author": "oneezy",
	"keywords": [
		"Svelte",
		"Sveltekit",
		"Turborepo",
		"PNPM",
		"Monorepo"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/oneezy/monorepo.git"
	},
	"main": "index.js",
	"license": "MIT",
	"workspaces": [
		"apps/*",
		"packages/**"
	],
	"scripts": {
		"preinstall": "pnpm exec inherit -u --unsafe-perm",
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"preview": "turbo run preview",
		"lint": "turbo run lint",
		"test": "turbo run test",
		"prepare": "husky install",
		"plop": "plop",
		"clean": "git clean -fXd"
	},
	"devDependencies": {
		"@changesets/cli": "^2.23.1",
		"@commitlint/cli": "^17.0.2",
		"@commitlint/config-conventional": "^17.0.2",
		"@packages/inherit": "workspace:^0.2.0",
		"@packages/minify": "workspace:^0.2.0",
		"autoprefixer": "^10.4.7",
		"husky": "^8.0.1",
		"lint-staged": "^12.5.0",
		"plop": "^3.1.0",
		"prettier": "^2.7.0",
		"turbo": "latest",
		"vite": "latest"
	},
	"type": "module",
	"dependencies": {
		"svelte-preprocess": "^4.10.6"
	}
}